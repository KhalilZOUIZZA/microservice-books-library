openssl pkcs12 -in ./backend-ms1/zyn/ssl/localhost/keystore.p12 -out keyStore.pem -nodes

openssl rsa -in keyStore.pem -out src/ssl/key.pem


openssl x509 -in keyStore.pem -out src/ssl/cert.pem


openssl req -x509 -newkey rsa:2048 -days 360 -nodes -keyout key.pem -out cert.pem -subj "/CN=localhost"
